{
  "printWidth": 125,
  "charset": "utf-8",
  "insertFinalNewline": false,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": true,
  "quoteProps": "as-needed",
  "jsxSingleQuote": true,
  "trailingComma": "none",
  "bracketSpacing": true,
  "jsxBracketSameLine": true,
  "arrowParens": "always",
  "singleAttributePerLine": false,
  "endOfLine": "crlf"
}
